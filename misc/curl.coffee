      when 72 then Error 'CURLE_TFTP_UNKNOWNID'
      when 73 then Error 'CURLE_REMOTE_FILE_EXISTS'
      when 74 then Error 'CURLE_TFTP_NOSUCHUSER'
      when 75 then Error 'CURLE_CONV_FAILED'
      when 76 then Error 'CURLE_CONV_REQD'
      when 77 then Error 'CURLE_SSL_CACERT_BADFILE'
      when 78 then Error 'CURLE_REMOTE_FILE_NOT_FOUND'
      when 79 then Error 'CURLE_SSH'
      when 80 then Error 'CURLE_SSL_SHUTDOWN_FAILED'
      when 81 then Error 'CURLE_AGAIN'
      when 82 then Error 'CURLE_SSL_CRL_BADFILE'
      when 83 then Error 'CURLE_SSL_ISSUER_ERROR'
      when 84 then Error 'CURLE_FTP_PRET_FAILED'
      when 85 then Error 'CURLE_RTSP_CSEQ_ERROR'
      when 86 then Error 'CURLE_RTSP_SESSION_ERROR'
      when 87 then Error 'CURLE_FTP_BAD_FILE_LIST'
      when 88 then Error 'CURLE_CHUNK_FAILED'
      when 89 then Error 'CURLE_NO_CONNECTION_AVAILABLE'
      else err
