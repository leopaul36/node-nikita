
module.exports =
  # See `man 3 libcurl-errors`
  error: (err) ->
    switch err.code
      when 1 then Error 'CURLE_UNSUPPORTED_PROTOCOL'
      when 2 then Error 'CURLE_FAILED_INIT'
      when 3 then Error 'CURLE_URL_MALFORMAT'
      when 4 then Error 'CURLE_NOT_BUILT_IN'
      when 5 then Error 'CURLE_COULDNT_RESOLVE_PROXY'
      when 6 then Error 'CURLE_COULDNT_RESOLVE_HOST'
      when 7 then Error 'CURLE_COULDNT_CONNECT'
      when 8 then Error 'CURLE_FTP_WEIRD_SERVER_REPLY'
      when 9 then Error 'CURLE_REMOTE_ACCESS_DENIED'
      when 10 then Error 'CURLE_FTP_ACCEPT_FAILED'
      when 11 then Error 'CURLE_FTP_WEIRD_PASS_REPLY'
      when 12 then Error 'CURLE_FTP_ACCEPT_TIMEOUT'
      when 13 then Error 'CURLE_FTP_WEIRD_PASV_REPLY'
      when 14 then Error 'CURLE_FTP_WEIRD_227_FORMAT'
      when 15 then Error 'CURLE_FTP_CANT_GET_HOST'
      when 17 then Error 'CURLE_FTP_COULDNT_SET_TYPE'
      when 18 then Error 'CURLE_PARTIAL_FILE'
      when 19 then Error 'CURLE_FTP_COULDNT_RETR_FILE'
      when 21 then Error 'CURLE_QUOTE_ERROR'
      when 22 then Error 'CURLE_HTTP_RETURNED_ERROR'
      when 23 then Error 'CURLE_WRITE_ERROR'
      when 25 then Error 'CURLE_UPLOAD_FAILED'
      when 26 then Error 'CURLE_READ_ERROR'
      when 27 then Error 'CURLE_OUT_OF_MEMORY'
      when 28 then Error 'CURLE_OPERATION_TIMEDOUT'
      when 30 then Error 'CURLE_FTP_PORT_FAILED'
      when 31 then Error 'CURLE_FTP_COULDNT_USE_REST'
      when 33 then Error 'CURLE_RANGE_ERROR'
      when 34 then Error 'CURLE_HTTP_POST_ERROR'
      when 35 then Error 'CURLE_SSL_CONNECT_ERROR'
      when 36 then Error 'CURLE_BAD_DOWNLOAD_RESUME'
      when 37 then Error 'CURLE_FILE_COULDNT_READ_FILE'
      when 38 then Error 'CURLE_LDAP_CANNOT_BIND'
      when 39 then Error 'CURLE_LDAP_SEARCH_FAILED'
      when 41 then Error 'CURLE_FUNCTION_NOT_FOUND'
      when 42 then Error 'CURLE_ABORTED_BY_CALLBACK'
      when 43 then Error 'CURLE_BAD_FUNCTION_ARGUMENT'
      when 45 then Error 'CURLE_INTERFACE_FAILED'
      when 47 then Error 'CURLE_TOO_MANY_REDIRECTS'
      when 48 then Error 'CURLE_UNKNOWN_OPTION'
      when 49 then Error 'CURLE_TELNET_OPTION_SYNTAX'
      when 51 then Error 'CURLE_PEER_FAILED_VERIFICATION'
      when 52 then Error 'CURLE_GOT_NOTHING'
      when 53 then Error 'CURLE_SSL_ENGINE_NOTFOUND'
      when 54 then Error 'CURLE_SSL_ENGINE_SETFAILED'
      when 55 then Error 'CURLE_SEND_ERROR'
      when 56 then Error 'CURLE_RECV_ERROR'
      when 58 then Error 'CURLE_SSL_CERTPROBLEM'
      when 59 then Error 'CURLE_SSL_CIPHER'
      when 60 then Error 'CURLE_SSL_CACERT'
      when 61 then Error 'CURLE_BAD_CONTENT_ENCODING'
      when 62 then Error 'CURLE_LDAP_INVALID_URL'
      when 63 then Error 'CURLE_FILESIZE_EXCEEDED'
      when 64 then Error 'CURLE_USE_SSL_FAILED'
      when 65 then Error 'CURLE_SEND_FAIL_REWIND'
      when 66 then Error 'CURLE_SSL_ENGINE_INITFAILED'
      when 67 then Error 'CURLE_LOGIN_DENIED'
